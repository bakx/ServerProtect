{
  "Plugins":
  [
    "windows.eventmonitor",
    "windows.firewall",
    "abuseip",
    "api.grpc",
    "livereport.signalr"
  ],
  "Api":
  {
    "Url": "your-api-url",
    "Token": "your-access-token"
  },
  "Blocking":
  {
    "Attempts": 3,
    "TimeSpanMinutes": 30,
    "DetectIPRange": true,
    "BlockIPRange": true,
    "UnblockTimeSpanMinutes": 300
  },
  "Tools":
  {
    "IPData":
    {
      "Url": "https://api.ipdata.co/{0}?api-key={1}",
      "Key": "your-api-key",
      "Enabled": false
    }
  }
}