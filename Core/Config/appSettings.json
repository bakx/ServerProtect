{
  "Plugins": [
    "eventmonitor",
    "abuseip"
  ],
  "Firewall": {
    "Rules": {
      "NameTemplate": "SP Service block {0}",
      "DescriptionTemplate": "Blocked on {0}",
      "DateFormat": "MM-dd-yyyy HH:m:s"
    }
  },
  "Blocking": {
    "Attempts": 3,
    "TimeSpanMinutes": 1440,
    "DetectIPRange": true,
    "BlockIPRange": true
  },
  "Tools": {
    "IPData": {
      "Url": "https://api.ipdata.co/1.1.1.1?api-key={0}",
      "Key": "your-key"
    }
  }
}