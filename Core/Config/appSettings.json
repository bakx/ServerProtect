{
  "Plugins":
  [
    "eventmonitor",
    "abuseip",
    "livereport.signalr"
  ],
  "Api":
  {
    "Url": "your-api-url",
    "Token": "your-access-token"
  },
  "Firewall":
  {
    "Rules":
    {
      "NameTemplate": "SP Service block {0}",
      "DescriptionTemplate": "Blocked on {0}",
      "DateFormat": "MM-dd-yyyy HH:m:s"
    }
  },
  "Blocking":
  {
    "Attempts": 3,
    "TimeSpanMinutes": 30,
    "DetectIPRange": true,
    "BlockIPRange": true,
    "UnblockTimeSpanMinutes": 300
  },
  "Tools":
  {
    "IPData":
    {
      "Url": "https://api.ipdata.co/{0}?api-key={1}",
      "Key": "your-api-key",
      "Enabled": false
    }
  }
}