{
  "Plugins":
  [
    "eventmonitor",
    "abuseip"
  ],
  "Firewall":
  {
    "Rules":
    {
      "NameTemplate": "SP Service block {0}",
      "DateFormat": "MM-dd-yyyy HH:m:s"
    }
  },
  "Blocking":
  {
    "Attempts": 3,
    "TimeSpanMinutes": 1440,
    "DetectIPRange": true,
    "BlockIPRange": true
  }
}